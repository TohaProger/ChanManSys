<?xml version="1.0" encoding="UTF-8"?>

<!--start window fxml file-->

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.chanmansys.StartApp"
            prefHeight="400.0" prefWidth="600.0">
    <Button layoutX="92.0" layoutY="56.0" mnemonicParsing="false" onAction="#onDataBase"
            style="-fx-background-color: white; -fx-border-color: black; -fx-border-radius: 10;" text="Загрузка из БД">
        <font>
            <Font size="14.0"/>
        </font>
    </Button>
    <Button layoutX="81.0" layoutY="166.0" mnemonicParsing="false" onAction="#onTxt"
            style="-fx-background-color: white; -fx-border-color: black; -fx-border-radius: 10;"
            text="Загрузка из Файла">
        <font>
            <Font size="14.0"/>
        </font>
    </Button>
    <Button layoutX="86.0" layoutY="112.0" mnemonicParsing="false" onAction="#onRam"
            style="-fx-background-color: white; -fx-border-color: black; -fx-border-radius: 10;" text="Загрузка из RAM">
        <font>
            <Font size="14.0"/>
        </font>
    </Button>
</AnchorPane>
